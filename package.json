{
  "name": "package-license-types",
  "version": "1.0.0",
  "description": "Extract license types from package.json data",
  "repository": "shinnn/package-license-types",
  "scripts": {
    "bower-install": "bower install --production",
    "pretest": "jscs *.js && eslint *.js && ${npm_package_scripts_bower_install}",
    "test": "node test.js",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/package-license-types/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js"
  ],
  "keywords": [
    "spdx",
    "data",
    "package",
    "license",
    "type",
    "browser",
    "client-side"
  ],
  "dependencies": {
    "is-spdx-license-identifier": "^2.0.1"
  },
  "devDependencies": {
    "bower": "^1.3.12",
    "eslint": "^0.10.2",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.8.1",
    "require-bower-files": "1.0.0",
    "tape": "^3.0.3"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "validateLineBreaks": "LF"
  }
}
