{
  "name": "package-license-types",
  "version": "1.0.0",
  "description": "Extract license types from package.json data",
  "repository": "shinnn/package-license-types",
  "scripts": {
    "bower-install": "bower install --production --force-latest",
    "pretest": "eslint --config node_modules/@shinnn/eslintrc/rc.json --ignore-path .gitignore . && ${npm_package_scripts_bower_install}",
    "test": "node test.js",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/package-license-types/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js"
  ],
  "keywords": [
    "spdx",
    "data",
    "package",
    "license",
    "type",
    "browser",
    "client-side"
  ],
  "dependencies": {
    "is-spdx-license-id": "^1.0.0"
  },
  "devDependencies": {
    "@shinnn/eslintrc": "^1.0.0",
    "bower": "^1.4.1",
    "eslint": "^0.20.0",
    "istanbul": "^0.3.13",
    "istanbul-coveralls": "^1.0.2",
    "require-bower-files": "2.0.0",
    "tape": "^4.0.0"
  }
}
